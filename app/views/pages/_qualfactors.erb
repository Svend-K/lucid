<div class="container">
  <div class="row">

    <% @qual_data_user_profile[0][:data].each do |factor, data| %>

      <div class="col-xs-12 col-md-6 qualitative-factor">
        <div class="table-image">

        <%= image_tag("icons8/#{data[:icon_name]}.png") %>
        <p><%= factor %></p>
        </div>
        <div class="progress-bars">
          <div class="progress">
            <% curr_city_score = data[:score] %>
            <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: <%= (curr_city_score*100).round(0).to_s %>%;">
            <%= (curr_city_score*100).round(0).to_s %>%
            </div>

          </div>
          <div class="progress">
            <% dest_city_score = @qual_data_user_profile[1][:data][factor][:score] %>
            <div class="progress-bar progress-bar-danger progress-bar-striped active" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: <%= (dest_city_score*100).round(0).to_s %>%;">
              <%= (dest_city_score*100).round(0).to_s %>%
            </div>

          </div>
        </div>
      </div>

    <% end %>

  </div>
</div>
